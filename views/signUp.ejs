<%- include("cabeceras/index") %>
<body>
    <div class="wrapper fadeInDown">
      <div id="formContent">
        <!-- Tabs Titles -->

        <!-- Login Form -->
        <form>
          <input
            type="text"
            id="name"
            class="fadeIn second"
            name="name"
            placeholder="name"
          />
          <input
            type="text"
            id="first_name"
            class="fadeIn second"
            name="firts_name"
            placeholder="firts_name"
          />
          <input
            type="text"
            id="last_name"
            class="fadeIn second"
            name="last_name"
            placeholder="last_name"
          />
          <input
            type="text"
            id="email"
            class="fadeIn second"
            name="email"
            placeholder="email"
          />
          <input
            type="text"
            id="password"
            class="fadeIn third"
            name="login"
            placeholder="password"
          /> 
          <button type="button" class="fadeIn fourth" value="Sign Up">
            <a href="#" onclick="signUp()">SignUp</a> 
         </button>
        </form>
        <div id="formFooter">
          <a class="underlineHover" href="http://localhost:4000/login">Already have an account?</a>
        </div>
      </div>
    </div>
    <script src="/js/signUp.js"></script>
  </body>
</html>